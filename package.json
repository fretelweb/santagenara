{
  "homepage": "https://fretelweb.github.io/santagenara",
  "name": "santagenara",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p watch:css react:start",
    "build": "react-scripts build",
    "react:start": "react-scripts start",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "build:css": "NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "deploy": "npm run build && gh-pages -d build && echo $npm_package_homepage",
    "prebuild": "npm run build:css"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "font-awesome": "^4.7.0",
    "gh-pages": "^3.1.0",
    "postcss": "^8.1.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "^2.0.1",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "postcss-nested": "^5.0.3"
  }
}
